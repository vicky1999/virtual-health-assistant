<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
 	<servlet>
 		<servlet-name>login</servlet-name>
 		<servlet-class>com.vhealth.auth.Login</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>login</servlet-name>
 		<url-pattern>/login</url-pattern>
 	</servlet-mapping>
  	<servlet>
 		<servlet-name>doctor-signup</servlet-name>
 		<servlet-class>com.vhealth.auth.DoctorSignUp</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>doctor-signup</servlet-name>
 		<url-pattern>/doctor-signup</url-pattern>
 	</servlet-mapping>
 	<servlet>
 		<servlet-name>lab-signup</servlet-name>
 		<servlet-class>com.vhealth.auth.LabSignUp</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>lab-signup</servlet-name>
 		<url-pattern>/lab-signup</url-pattern>
 	</servlet-mapping>
 	<servlet>
 		<servlet-name>user-signup</servlet-name>
 		<servlet-class>com.vhealth.auth.UserSignUp</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>user-signup</servlet-name>
 		<url-pattern>/user-signup</url-pattern>
 	</servlet-mapping>
	<servlet>
		<servlet-name>send-mail</servlet-name>
		<servlet-class>mail.MailHandler</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>send-mail</servlet-name>
  		<url-pattern>/send-mail</url-pattern>
  	</servlet-mapping>
  		<servlet>
		<servlet-name>send-report</servlet-name>
		<servlet-class>mail.MailHandler</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>send-report</servlet-name>
  		<url-pattern>/send-report</url-pattern>
  	</servlet-mapping>
  	
</web-app>